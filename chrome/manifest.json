{
    "manifest_version": 3,
    "name": "Alerta de Contraseña",
    "description": "La extensión Alerta de Contraseña detecta contraseñas ingresadas en páginas no autorizadas.",
    "version": "2.0.4",
    "default_locale": "en",
    "icons": 
      {
        "128": "icon128.png"
      }
    ,
    "host_permissions": [
      "<all_urls>"
    ],
    "permissions": [
      "identity",
      "notifications",
      "storage",
      "tabs",
      "scripting"
    ],
    "content_scripts": [
      {
        "matches": ["<all_urls>"],
        "js": ["content_script_compiled.js"],
        "all_frames": true,
        "run_at": "document_start"
      }
    ],
    "background": {
      "service_worker": "background_compiled.js"
    },
    "content_security_policy": {
      "extension_pages": "script-src 'self'; object-src 'self'",
      "sandbox": "sandbox allow-scripts; default-src 'none'; script-src 'self'"
    },
    "oauth2": {
      "client_id": "897749729682-2j2fjtnfde4kgi40fvjrp7ude48ooh4n.apps.googleusercontent.com",
      "scopes": ["email"]
    }
  }
  